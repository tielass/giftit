<%= render "shared/giftheader" %>
<%= render "shared/calendar" %>
<div class="container text-center">
  <%= link_to "Add event", new_event_path, class:"btn-giftit my-5" %>
</div>
 <div class="container text-center">
  <% @events.each do |event| %>
  <p><%= event.name %>on: <%= event.start_time.strftime("%a %e %b %y") %></p>
  <% end %>
</div>
